  <process id="addBill" name="加班流程" isExecutable="true">
    <startEvent id="startevent1" name="Start"></startEvent>
    <userTask id="master" name="主管" activiti:assignee="${master}"></userTask>
    <userTask id="leader" name="领导" activiti:candidateUsers="${leaders}"></userTask>
    <sequenceFlow id="flow1" sourceRef="exclusivegateway1" targetRef="leader">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[#{isMaster==1}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="boss" name="老板" activiti:assignee="${bosses}">
      <extensionElements>
        <activiti:executionListener event="start" class="com.kim.activiti.spring.boot.executionListener.AddWorkBillStartExecutionListener"></activiti:executionListener>
        <activiti:executionListener event="end" class="com.kim.activiti.spring.boot.executionListener.AddWorkBillEndExecutionListener"></activiti:executionListener>
        <activiti:taskListener event="create" class="com.kim.activiti.spring.boot.tasklistener.BossCreateTaskListener"></activiti:taskListener>
        <activiti:taskListener event="assignment" class="com.kim.activiti.spring.boot.tasklistener.BossAssignmentTaskListener"></activiti:taskListener>
        <activiti:taskListener event="complete" class="com.kim.activiti.spring.boot.tasklistener.BossCompletionTaskListener"></activiti:taskListener>
      </extensionElements>
      <multiInstanceLoopCharacteristics isSequential="true" activiti:collection="${bossList}" activiti:elementVariable="bosses"></multiInstanceLoopCharacteristics>
    </userTask>
    <sequenceFlow id="flow2" sourceRef="exclusivegateway2" targetRef="boss">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[#{isLeader==1}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow3" sourceRef="master" targetRef="exclusivegateway1"></sequenceFlow>
    <userTask id="create" name="提交人" activiti:assignee="${提交人}"></userTask>
    <sequenceFlow id="flow4" sourceRef="exclusivegateway1" targetRef="create">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[#{isMaster==0}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow5" sourceRef="startevent1" targetRef="master"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow7" sourceRef="leader" targetRef="exclusivegateway2"></sequenceFlow>
    <sequenceFlow id="flow8" sourceRef="exclusivegateway2" targetRef="create">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[#{isLeader==0}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="exclusivegateway3" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow9" sourceRef="boss" targetRef="exclusivegateway3"></sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow10" sourceRef="exclusivegateway3" targetRef="create">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[#{isBoss==0}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow11" sourceRef="exclusivegateway3" targetRef="endevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[#{isBoss==1}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow12" sourceRef="create" targetRef="master"></sequenceFlow>
  </process>
