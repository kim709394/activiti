  <process id="leaveBill" name="离职流程" isExecutable="true">
    <extensionElements>
      <activiti:executionListener event="start" class="com.kim.activiti.spring.boot.executionListener.AddWorkBillStartExecutionListener"></activiti:executionListener>
      <activiti:executionListener event="end" class="com.kim.activiti.spring.boot.executionListener.LeaveBillEndListener"></activiti:executionListener>
    </extensionElements>
    <startEvent id="startevent1" name="Start"></startEvent>
    <userTask id="manager" name="经理" activiti:assignee="${managers}">
      <multiInstanceLoopCharacteristics isSequential="false" activiti:collection="${managerList}" activiti:elementVariable="managers">
        <completionCondition>${managerCount==1}</completionCondition>
      </multiInstanceLoopCharacteristics>
    </userTask>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="manager"></sequenceFlow>
    <userTask id="president" name="总裁" activiti:assignee="${presidents}">
      <multiInstanceLoopCharacteristics isSequential="false" activiti:collection="${presidentList}" activiti:elementVariable="presidents">
        <completionCondition>${presidentCount==2}</completionCondition>
      </multiInstanceLoopCharacteristics>
    </userTask>
    <sequenceFlow id="flow2" sourceRef="exclusivegateway1" targetRef="president">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[#{isManager==1}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <userTask id="creator" name="提交人" activiti:assignee="${creator}"></userTask>
    <sequenceFlow id="flow5" sourceRef="exclusivegateway1" targetRef="creator">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[#{isManager==0}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow6" sourceRef="creator" targetRef="manager"></sequenceFlow>
    <parallelGateway id="parallelgateway1" name="Parallel Gateway"></parallelGateway>
    <sequenceFlow id="flow7" sourceRef="president" targetRef="parallelgateway1"></sequenceFlow>
    <userTask id="boss" name="老板1" activiti:assignee="${boss1s}">
      <multiInstanceLoopCharacteristics isSequential="true" activiti:collection="${boss1List}" activiti:elementVariable="boss1s">
        <completionCondition>${boss1Count==2}</completionCondition>
      </multiInstanceLoopCharacteristics>
    </userTask>
    <sequenceFlow id="flow8" sourceRef="parallelgateway1" targetRef="boss">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[#{isPresident==1}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="boss2" name="老板2" activiti:assignee="${boss2}"></userTask>
    <sequenceFlow id="flow9" sourceRef="parallelgateway1" targetRef="boss2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[#{isPresident==2}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow10" sourceRef="boss" targetRef="creator">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[#{isBoss1==0}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow11" sourceRef="boss" targetRef="endevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[#{isBoss1==1}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow12" sourceRef="boss2" targetRef="creator">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[#{isBoss2==0}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow13" sourceRef="boss2" targetRef="endevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[#{isBoss2==1}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow14" sourceRef="manager" targetRef="exclusivegateway1"></sequenceFlow>
  </process>
